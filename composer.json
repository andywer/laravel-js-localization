{
    "name": "jamoliddin-firdavs/js-localization",
    "description": "Laravel package to provide localizations to the Javascript code.",
    "keywords": ["laravel", "javascript", "localization", "translation", "messages"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Wermke",
            "email": "andy@dev.next-step-software.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "6.*|7.*|8.*|9.*|10.*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "4.*",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "JsLocalization\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 7", "Laravel 8", "Laravel 9", "Laravel 10"]
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ]
}
